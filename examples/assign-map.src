my_map = "empty"; // assign default value

my_map # ["eddie", "van halen", ] = "musician";
my_map # ["eddie", "murphy", ] = "comedian";

assert(== "comedian" my_map # ["eddie", "murphy", ]);
assert(== "musician" my_map # ["eddie", "van halen", ]);
assert(== "empty" my_map # ["eddie", "corrigall", ]);
